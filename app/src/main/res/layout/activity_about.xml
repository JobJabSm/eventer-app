<?xml version="1.0" encoding="utf-8"?>
<com.eventer.app.AboutPage.ElasticDragDismissFrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/draggable_frame"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:dragDismissDistance="112dp"
    app:dragDismissScale="0.95"
    tools:context="com.eventer.app.AboutPage.AboutActivity_PostLollipop"
    >

    <android.support.v4.view.ViewPager
        android:id="@+id/pager"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.pixelcan.inkpageindicator.InkPageIndicator
        android:id="@+id/indicator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="top|center_horizontal"
        android:layout_marginTop="16dp"
        app:pageIndicatorColor="@color/page_indicator_dark"
        app:currentPageIndicatorColor="@color/page_indicator_dark_selected" />

</com.eventer.app.AboutPage.ElasticDragDismissFrameLayout>
